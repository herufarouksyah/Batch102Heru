@model IEnumerable<waMvc5Day13.Models.Fakultas>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Add")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.KodeFakultas)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NamaFakultas)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.KodeFakultas)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NamaFakultas)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Hapus", new { id=item.Id })
        </td>
    </tr>
}

</table>
